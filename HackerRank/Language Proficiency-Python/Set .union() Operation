# Defining the main function
if __name__ == '__main__' :
    # Getting all the inputs
    n = int(input())
    N = [int(i) for i in input().split()][:n]
    b = int(input())
    B = [int(i) for i in input().split()][:b]
    # Converting 'list()' to 'set()' to perform the 'union()' operation
    N = set(N)
    B = set(B)
    # Logic to perform the 'union()' operation on the sets 'N' & 'B'
    unioned = N.union(B)
    print(len(unioned))
